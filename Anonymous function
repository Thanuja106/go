package main

import "fmt"
var f=func (s string){
fmt.Println("h"+s)
}
func main() {
  f("ello")
}
OUTPUT:
hello
/---------------------------------------------------------------------------------------------------
package main

import "fmt"

func main() {
  fs:=func(f string){
    fmt.Println(f)
  }
  fs("hai")

}
OUTPUT:
hai
/--------------------------------------------------------------------------------------------------------
package main

import "fmt"

func main() {
 f:=func (a int,b int)int{
     fmt.Println("2rd")---------------->explaination how it flows
   return a+b
 }
 s(f)
}
func s(f func(a,b int)int){
  fmt.Println("1st")
res:=f(56,7)
  fmt.Println("3rd")
fmt.Println(res)
}
OUTPUT:
1st
3rd
2nd
63
